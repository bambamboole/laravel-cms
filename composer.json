{
    "name": "bambamboole/laravel-cms",
    "description": "",
    "keywords": [
        "bambamboole",
        "laravel-cms"
    ],
    "homepage": "https://github.com/bambamboole/laravel-cms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Manuel Christlieb",
            "email": "manuel@christlieb.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "inertiajs/inertia-laravel": "^0.2.5",
        "laravel/framework": "^7.0",
        "laravel/sanctum": "^2.0",
        "spatie/commonmark-highlighter": "^2.1",
        "spatie/laravel-sluggable": "^2.4"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5",
        "sixlive/json-schema-assertions": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Bambamboole\\LaravelCms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bambamboole\\LaravelCms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bambamboole\\LaravelCms\\LaravelCmsServiceProvider"
            ]
        }
    }
}
