{
    "name": "oscer-io/oscer",
    "description": "",
    "keywords": [
        "laravel",
        "cms",
        "blog",
        "bambamboole"
    ],
    "homepage": "https://oscer.io",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Manuel Christlieb",
            "email": "manuel@christlieb.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "laravel/framework": "^7.0",
        "laravel/sanctum": "^2.0",
        "spatie/commonmark-highlighter": "^2.1",
        "spatie/laravel-permission": "^3.11",
        "spatie/laravel-sluggable": "^2.4",
        "symfony/yaml": "^5.0",
        "tightenco/ziggy": "^0.9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5",
        "sixlive/json-schema-assertions": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Oscer\\Cms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oscer\\Cms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oscer\\Cms\\OscerServiceProvider"
            ]
        }
    }
}
